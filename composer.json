{
  "name": "bfitech/zapstore-mysql",
  "description": "MySQL support for zapstore.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rendra Suroso",
      "email": "rs@cogsci.bandungfe.net"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5 || ~7.0",
    "ext-mysqlnd": "*",
    "ext-pdo_mysql": "*",
    "bfitech/zapstore": "dev-master as 1.2.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "BFITech\\ZapStore\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "vendor/bfitech/zapstore/"
    }
  }
}
